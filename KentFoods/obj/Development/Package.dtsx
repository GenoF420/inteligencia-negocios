        <SQLTask:SqlTaskData
          SQLTask:Connection="{9478CDF1-90A6-4A3B-945D-0CFE4668C538}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="-- 1. Dimensin de Cargos&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Cargos' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Cargos]&#xA;    (&#xA;        [CargoID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Cargo]   NVARCHAR(50) NOT NULL&#xA;    );&#xA;END;&#xA;&#xA;-- 2. Dimensin de Categoras&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Categorias' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Categorias]&#xA;    (&#xA;        [CategoriaID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Categoria]   NVARCHAR(100) NOT NULL,&#xA;        [Descripcion] NVARCHAR(255)&#xA;    );&#xA;END;&#xA;&#xA;-- 3. Dimensin de Contactos&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Contactos' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Contactos]&#xA;    (&#xA;        [ContactoID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Contacto]   NVARCHAR(100) NOT NULL,&#xA;        [CargoID]    INT NULL,&#xA;        CONSTRAINT [FK_Contactos_Cargos] FOREIGN KEY ([CargoID])&#xA;            REFERENCES [Dim_Cargos]([CargoID])&#xA;    );&#xA;END;&#xA;&#xA;-- 4. Dimensin de Pases&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Paises' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Paises]&#xA;    (&#xA;        [PaisID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Pais]   NVARCHAR(100) NOT NULL&#xA;    );&#xA;END;&#xA;&#xA;-- 5. Dimensin de Ciudades&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Ciudades' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Ciudades]&#xA;    (&#xA;        [CiudadID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Ciudad]   NVARCHAR(100) NOT NULL,&#xA;        [PaisID]   INT NULL,&#xA;        CONSTRAINT [FK_Ciudades_Paises] FOREIGN KEY ([PaisID])&#xA;            REFERENCES [Dim_Paises]([PaisID])&#xA;    );&#xA;END;&#xA;&#xA;-- 6. Dimensin de Clientes&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Clientes' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Clientes]&#xA;    (&#xA;        [ClienteID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Empresa]   NVARCHAR(100) NOT NULL,&#xA;        [Contacto]  NVARCHAR(100),&#xA;        [CargoID]   INT NULL,&#xA;        [Direccion] NVARCHAR(150),&#xA;        [CiudadID]  INT NULL,&#xA;        [CodigoPostal] NVARCHAR(10),&#xA;        [Telefono]  NVARCHAR(24),&#xA;        CONSTRAINT [FK_Clientes_Cargos] FOREIGN KEY ([CargoID])&#xA;            REFERENCES [Dim_Cargos]([CargoID]),&#xA;        CONSTRAINT [FK_Clientes_Ciudades] FOREIGN KEY ([CiudadID])&#xA;            REFERENCES [Dim_Ciudades]([CiudadID])&#xA;    );&#xA;    ALTER TABLE [Dim_Clientes]&#xA;    ADD [OriginalClienteID] NVARCHAR(5) NULL;&#xA;END;&#xA;&#xA;-- 7. Dimensin de Empleados&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Empleados' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Empleados]&#xA;    (&#xA;        [EmpleadoID]        INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Apellido]          NVARCHAR(100),&#xA;        [Nombre]            NVARCHAR(100),&#xA;        [CargoID]           INT NULL,&#xA;        [FechaNacimiento]   DATE,&#xA;        [FechaContratacion] DATE,&#xA;        [Direccion]         NVARCHAR(150),&#xA;        [CiudadID]          INT NULL,&#xA;        [CodigoPostal]      NVARCHAR(10),&#xA;        [Telefono]          NVARCHAR(24),&#xA;        CONSTRAINT [FK_Empleados_Cargos] FOREIGN KEY ([CargoID])&#xA;            REFERENCES [Dim_Cargos]([CargoID]),&#xA;        CONSTRAINT [FK_Empleados_Ciudades] FOREIGN KEY ([CiudadID])&#xA;            REFERENCES [Dim_Ciudades]([CiudadID])&#xA;    );&#xA;END;&#xA;&#xA;-- 8. Dimensin de Historial Laboral&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_HistorialLaboral' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_HistorialLaboral]&#xA;    (&#xA;        [HistorialID]  INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [EmpleadoID]   INT NOT NULL,&#xA;        [CargoID]      INT NOT NULL,&#xA;        [FechaCambio]  DATE,&#xA;        [MotivoCambio] NVARCHAR(255),&#xA;        CONSTRAINT [FK_HistorialLaboral_Empleados] FOREIGN KEY ([EmpleadoID])&#xA;            REFERENCES [Dim_Empleados]([EmpleadoID]),&#xA;        CONSTRAINT [FK_HistorialLaboral_Cargos] FOREIGN KEY ([CargoID])&#xA;            REFERENCES [Dim_Cargos]([CargoID])&#xA;    );&#xA;END;&#xA;&#xA;-- 9. Dimensin de Proveedores&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Proveedores' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Proveedores]&#xA;    (&#xA;        [ProveedorID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Proveedor]   NVARCHAR(100) NOT NULL,&#xA;        [Contacto]    NVARCHAR(100),&#xA;        [CargoID]     INT NULL,&#xA;        [Direccion]   NVARCHAR(150),&#xA;        [CiudadID]    INT NULL,&#xA;        [CodigoPostal] NVARCHAR(10),&#xA;        [Telefono]    NVARCHAR(24),&#xA;        CONSTRAINT [FK_Proveedores_Cargos] FOREIGN KEY ([CargoID])&#xA;            REFERENCES [Dim_Cargos]([CargoID]),&#xA;        CONSTRAINT [FK_Proveedores_Ciudades] FOREIGN KEY ([CiudadID])&#xA;            REFERENCES [Dim_Ciudades]([CiudadID])&#xA;    );&#xA;END;&#xA;&#xA;-- 10. Dimensin de Productos&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Productos' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Productos]&#xA;    (&#xA;        [ProductoID]        INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Producto]          NVARCHAR(100) NOT NULL,&#xA;        [CategoriaID]       INT NULL,&#xA;        [ProveedorID]       INT NULL,&#xA;        [CantidadPorUnidad] NVARCHAR(50),&#xA;        [PrecioUnitario]    DECIMAL(10, 2),&#xA;        CONSTRAINT [FK_Productos_Categorias] FOREIGN KEY ([CategoriaID])&#xA;            REFERENCES [Dim_Categorias]([CategoriaID]),&#xA;        CONSTRAINT [FK_Productos_Proveedores] FOREIGN KEY ([ProveedorID])&#xA;            REFERENCES [Dim_Proveedores]([ProveedorID])&#xA;    );&#xA;END;&#xA;&#xA;-- 11. Dimensin de Tiempo&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Tiempo' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Tiempo]&#xA;    (&#xA;        [TiempoID]  INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Fecha]     DATE NOT NULL,&#xA;        [Dia]       INT,&#xA;        [Mes]       INT,&#xA;        [Trimestre] INT,&#xA;        [Ao]       INT,&#xA;        [DiaSemana] NVARCHAR(20),&#xA;        [MesNombre] NVARCHAR(20)&#xA;    );&#xA;END;&#xA;&#xA;-- 12. Dimensin de Transportistas&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_Transportistas' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_Transportistas]&#xA;    (&#xA;        [TransportistaID] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Transportista]   NVARCHAR(100) NOT NULL,&#xA;        [Telefono]        NVARCHAR(24)&#xA;    );&#xA;END;&#xA;&#xA;-- 13. Dimensin de Turnos de Trabajo&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_TurnosTrabajo' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_TurnosTrabajo]&#xA;    (&#xA;        [TurnoID]     INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Descripcion] NVARCHAR(100),&#xA;        [HoraInicio]  TIME,&#xA;        [HoraFin]     TIME&#xA;    );&#xA;END;&#xA;&#xA;-- 14. Dimensin de Unidades de Medida&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Dim_UnidadesMedida' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Dim_UnidadesMedida]&#xA;    (&#xA;        [UnidadID]    INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Descripcion] NVARCHAR(100) NOT NULL&#xA;    );&#xA;END;&#xA;&#xA;-- 15. Tabla de Hechos de Ventas&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Fact_Ventas' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Fact_Ventas]&#xA;    (&#xA;        [VentaID]         INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [OrdenID]         INT NOT NULL,&#xA;        [ProductoID]      INT NOT NULL,&#xA;        [ClienteID]       INT NOT NULL,&#xA;        [EmpleadoID]      INT NOT NULL,&#xA;        [TransportistaID] INT NOT NULL,&#xA;        [TiempoID]        INT NOT NULL,&#xA;        [Cantidad]        INT,&#xA;        [PrecioUnitario]  DECIMAL(10, 2),&#xA;        [Descuento]       DECIMAL(5, 2),&#xA;        [MontoVendido]    DECIMAL(18, 2),&#xA;        [MontoDescuento]  DECIMAL(18, 2),&#xA;        [MontoPagado]     DECIMAL(18, 2),&#xA;        [FechaOrden]      DATE,&#xA;        [FechaEnvio]      DATE,&#xA;        CONSTRAINT [FK_FactVentas_Productos] FOREIGN KEY ([ProductoID])&#xA;            REFERENCES [Dim_Productos]([ProductoID]),&#xA;        CONSTRAINT [FK_FactVentas_Clientes] FOREIGN KEY ([ClienteID])&#xA;            REFERENCES [Dim_Clientes]([ClienteID]),&#xA;        CONSTRAINT [FK_FactVentas_Empleados] FOREIGN KEY ([EmpleadoID])&#xA;            REFERENCES [Dim_Empleados]([EmpleadoID]),&#xA;        CONSTRAINT [FK_FactVentas_Transportistas] FOREIGN KEY ([TransportistaID])&#xA;            REFERENCES [Dim_Transportistas]([TransportistaID]),&#xA;        CONSTRAINT [FK_FactVentas_Tiempo] FOREIGN KEY ([TiempoID])&#xA;            REFERENCES [Dim_Tiempo]([TiempoID])&#xA;    );&#xA;END;&#xA;&#xA;-- Log Table&#xA;IF NOT EXISTS (SELECT * FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_NAME = 'Logs' AND TABLE_SCHEMA = 'dbo')&#xA;BEGIN&#xA;    CREATE TABLE [Logs]&#xA;    (&#xA;        [LogId] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,&#xA;        [Time] DATETIME NOT NULL DEFAULT GETDATE(),&#xA;        [LogType] VARCHAR(24) NOT NULL DEFAULT 'Info',&#xA;        [Message] VARCHAR(MAX) NOT NULL&#xA;    );&#xA;END;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
